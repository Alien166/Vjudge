#include<bits/stdc++.h>
using namespace std;
int main()
{
    string str; cin>>str;
    int Up=0,Do=0;
    char ch;
    for(int i=0;i<str.size();i++)
    {
        if(isupper(str[i]))
        {
            Up++;
        }
        else
        {
            Do++;
        }

    }
    if(Up>Do)
    {
        for(int i=0;i<str.size();i++)
        {
            ch=toupper(str[i]);
            cout<<ch;
        }
    }
    else
    {
        for(int i=0;i<str.size();i++)
        {
            ch=tolower(str[i]);
            cout<<ch;
        }
    }

    return 0;
}
